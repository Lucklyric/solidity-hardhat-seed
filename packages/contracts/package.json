{
  "name": "hardhat-seed-contracts",
  "version": "0.4.0",
  "description": "Hardhat seed template",
  "main": "typechain/index.js",
  "scripts": {
    "build": "hardhat compile",
    "lint": "eslint \"**/*.{js,ts}\" && solhint contracts/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix contracts/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile",
    "test:cov": "hardhat coverage --network hardhat"
  },
  "author": "Alvin(Xinyao) Sun <lucklyric@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/networks": "^5.4.2",
    "@ethersproject/providers": "^5.4.4",
    "@ethersproject/solidity": "^5.4.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@openzeppelin/contracts": "^4.3.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.0",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "@openzeppelin/test-helpers": "^0.5.13",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/ethereumjs-util": "^6.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.2",
    "@types/pino": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "chai-ethers": "^0.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.0",
    "ethers": "^5.4.5",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.11",
    "hardhat-docgen": "^1.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "pino": "^6.13.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "chalk": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint",
    "*.sol": "solhint",
    "*.{ts,js,sol}": "prettier --check"
  }
}
